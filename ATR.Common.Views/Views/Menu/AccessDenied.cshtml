@model ATR.Common.Models.AccessDeniedViewModel

@{
    ViewBag.Title = "Access Denied";
}

<div id="waitMessage" class="container text-center">
    <div class="form-group row">
        <div class="col-12">
            <h3>Sorry, you don’t have access.</h3>
        </div>
    </div>
    <div class="form-group row">
        <div class="col-12">
            @Html.Raw(@Model.ErrorMessage)
        </div>
    </div>
</div>

